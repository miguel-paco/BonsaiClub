@returns(float)
def process(value):
    #Load Global Variables
    global PhotoSwitch, PhotoCounter, Trial

    if Trial is not None:
      if Trial != value.Item2:
        PhotoCounter = None
    Trial = value.Item2

    if PhotoCounter is None:
      PhotoCounter = value.Item1
      Switch = 1
    if PhotoCounter &gt; 0:
      PhotoSwitch = 1
      PhotoCounter-=1
